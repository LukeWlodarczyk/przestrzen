---
import BaseLayout from "@layouts/BaseLayout.astro";

import { STATIC_ROUTES } from "@routes";

import type { Office } from "@features/reservation/types";

interface Props {
  office: Office;
}

const { office } = Astro.props;
---

<BaseLayout
  currentPage={STATIC_ROUTES.reservation}
  fontsToPreload={[
    "poppins-200",
    "poppins-300",
    "poppins-500",
    "poppins-regular",
  ]}
  metaDescription={office.metaDescription}
  metaTitle={office.metaTitle}
>
  <slot />
</BaseLayout>
